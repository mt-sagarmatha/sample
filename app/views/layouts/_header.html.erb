<header>
	<%=link_to logo, root_path %>
	<nav class = "round">
		<ul>
			<li><%= link_to "home", root_path %></li>
			<% if signed_in?%>
				<li><%= link_to "Profile", user_path(current_user) %></li>
			<% end %>	
			<li><%= link_to "help", help_path %></li>
			<% if signed_in?%>
				<li><%= link_to "Sign out", signout_path %></li>
			<% else %>
				<li><%= link_to "Sign in", signin_path %></li>
			<% end %>
		</ul>
	</nav>
</header>
